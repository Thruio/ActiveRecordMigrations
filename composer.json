{
  "name"       : "thru.io/active_record_migrations",
  "description": "Migrations for Active Record",
  "authors"    : [
    {
      "name" : "Matthew Baggett",
      "email": "matthew@baggett.me"
    }
  ],
  "require"    : {
    "thru.io/active_record": "0.2.*",
    "nategood/commando": "*",
    "symfony/yaml": "2.6.6"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "codeclimate/php-test-reporter": "dev-master",
    "fzaninotto/faker": "1.5.*@dev"
  },
  "autoload"   : {
    "psr-4": {
      "Thru\\ActiveRecordMigrations\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thru\\ActiveRecordMigrations\\Test\\": "tests/"
    }
  },
  "bin": [
    "ar_migrate"
  ]
}
